#compdef gfl

local -a _1st_arguments

git_branch_names () {
  branch_names=(${${(f)"$(_call_program branchrefs git for-each-ref --format='"%(refname)"' refs/heads 2>/dev/null)"}#refs/heads/})
}

local expl

if (( CURRENT == 2 )); then
    git_branch_names

    _wanted branch_names expl "branch_names" compadd $* - $branch_names
    return 0
fi