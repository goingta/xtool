#mobileprovision tools
	
function mpinstall(){
    sh $HOME/xtool/others/ios/mptools/mpinstall.sh $1
}

function mputil(){
    sh $HOME/xtool/others/ios/mptools/mputil.sh $1 $2
}

function mpclean(){
    sh $HOME/xtool/others/ios/mptools/mpclean.sh
}

function mplist(){
    sh $HOME/xtool/others/ios/mptools/mplist.sh
}

function mpcheck(){
	sh $HOME/xtool/others/ios/mptools/mpcheck.sh $1
}

function mpfix(){
	sh $HOME/xtool/others/ios/mptools/mpfix.sh
}

alias mpinstall='mpinstall'
alias mputil='mputil'
alias mpclean='mpclean'
alias mplist='mplist'
alias mpcheck='mpcheck'
alias mpfix='mpfix'